<% _.each(options, function(option, index) { %>
  <h3><%= option.name %></h3>
  <ul id="<%= 'option' + (index + 1) %>" data-name="<%= option.name %>">
    <% _.each(_.sortBy(option.values, function(option) { return parseFloat(option) }), function(value) { %>
      <li data-value="<%= value %>"><%= value %></li>
    <% }); %>
  </ul>
<% }); %>

